import java.io.*;
import java.util.*;
public class Solution {
public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int i;
        int n=s.nextInt();
   int a[]=new int[n];
    for(i=0;i<n;i++)
    {
        a[i]=s.nextInt();
    }
    int sum=0;
    for(i=0;i<n;i++)
    {
        sum=sum+a[i];
        System.out.printf(sum+" ");
    }
    }
}
    
