import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int n=s.nextInt();
        int i,count;
        count=1;
        for(i=1;i<n;i++)
        {
            if(n%i==0)
                count++;
        }
        if(count==2)
          System.out.printf("%.2f",Math.sqrt(n));
           else
           System.out.printf(n+" is not a prime no");
    }
}
