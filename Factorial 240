import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
      Scanner s=new Scanner (System.in);
        int n=s.nextInt();
        int a=(n-1)%9+1;
           int fact,i;
        fact=1;
        for(i=1;i<=a;i++)
        {
            fact=fact*i;
        }
        System.out.println(fact);
    }
}
